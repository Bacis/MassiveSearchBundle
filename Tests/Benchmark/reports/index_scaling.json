{
    "rows": [
        {
            "cells": [
                {
                    "name": "adapter",
                    "expr": "class_name(string(ancestor::benchmark/@class))"
                },
                {
                    "class": "microsecond",
                    "name": "{{ cell.item }}",
                    "expr": "number(./variant[parameter[@name='nb_documents']/@value = {{ cell.item }}]//iteration/@time) div 1000000",
                    "with_items": ["10", "100", "1000"]
                }
            ],
            "with_query": ".//subject[group[@name='{{ param.group }}']]"
        }
    ],
    "params": {"group": "index"},
    "classes": {
        "microsecond": [
            [ "number", {"decimal_places": 4 }],
            [ "printf", {"format": "%s<comment>s</comment>"}]
        ]
    }
}
